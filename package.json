{
  "name": "undici-cache-redis",
  "version": "1.0.1",
  "description": "An Undici cache store with a Redis backend.",
  "homepage": "https://github.com/platformatic/undici-cache-redis#readme",
  "author": "Platformatic Inc. <oss@platformatic.dev> (https://platformatic.dev)",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/platformatic/undici-cache-redis.git"
  },
  "keywords": [
    "undici",
    "cache",
    "redis"
  ],
  "bugs": {
    "url": "https://github.com/platformatic/undici-cache-redis/issues"
  },
  "private": false,
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "type": "module",
  "exports": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.base.json",
    "lint": "eslint --cache",
    "typecheck": "tsc -p . --noEmit",
    "format": "prettier -w benchmarks src test",
    "test": "c8 -c test/config/c8-local.json node --test --test-reporter=cleaner-spec-reporter --test-timeout=300000 'test/**/*.test.ts'",
    "test:ci": "c8 -c test/config/c8-ci.json node --test --test-reporter=cleaner-spec-reporter --test-timeout=300000 'test/**/*.test.ts'",
    "test:integration": "node --test --test-reporter=cleaner-spec-reporter --test-timeout=300000 'test/**/*.test.js'",
    "test:docker:up": "docker-compose up -d --wait",
    "test:docker:down": "docker-compose down",
    "ci": "npm run build && npm run lint && npm run test:ci && npm run test:integration",
    "prepublishOnly": "npm run build && npm run lint",
    "postpublish": "git push origin && git push origin -f --tags",
    "example:server": "node example/server.ts",
    "example:client": "node example/client.ts",
    "example:advanced": "node example/advanced-client.ts",
    "example:docker:up": "cd example && docker-compose up -d",
    "example:docker:down": "cd example && docker-compose down",
    "example:run": "cd example && ./run-example.sh",
    "bench": "./benchmarks/run-benchmarks.sh"
  },
  "dependencies": {
    "hyperid": "^3.3.0",
    "iovalkey": "^0.3.3",
    "lru_map": "^0.4.1",
    "p-map": "^7.0.4",
    "xxhash-wasm": "^1.1.0"
  },
  "devDependencies": {
    "@platformatic/tsconfig": "^0.2.0",
    "@types/atomic-sleep": "^1.0.1",
    "@types/autocannon": "^7.12.7",
    "@types/node": "^25.0.2",
    "@types/semver": "^7.7.1",
    "atomic-sleep": "^1.0.0",
    "autocannon": "^8.0.0",
    "c8": "^10.1.3",
    "cleaner-spec-reporter": "^1.0.3",
    "eslint": "^9.39.2",
    "fastify": "^5.6.2",
    "neostandard": "^0.12.2",
    "prettier": "^3.7.4",
    "prettier-plugin-space-before-function-paren": "^0.0.10",
    "semver": "^7.7.3",
    "typescript": "^5.9.3",
    "undici": "^7.16.0"
  },
  "engines": {
    "node": ">= 22.19.0"
  }
}
